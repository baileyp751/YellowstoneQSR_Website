---
import BaseHead from '../components/BaseHead.astro';
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <BaseHead title="Ol' Faithful" description="Private Video Page" />
    <style>
      html, body {
        margin: 0;
        padding: 0;
        height: 100%;
        background-color: black;
      }

      iframe {
        position: absolute;
        top: 0;
        left: 0;
        width: 100vw;
        height: 100vh;
        border: none;
      }
    </style>
  </head>
  <body>
    <iframe
  width="100%"
  height="100%"
  src="https://www.youtube.com/embed/NUh7jIxvh2Q?autoplay=1&mute=1&vq=hd1080&controls=0&rel=0&modestbranding=1"
  frameborder="0"
  allow="autoplay; encrypted-media; fullscreen"
  allowfullscreen
></iframe>
  </body>
</html>
